<div class="container">
    <h2>Xabarlar</h2>
    <table class="table table-striped table-centered mb-0">
        <thead>
            <tr>
                <!-- <th>Rasm</th> -->
                <th>Ism</th>
                <th>Telefon</th>
                <th>Xabar</th>
                <th>Sana</th>
            </tr>
        </thead>
        <tbody>
            <% for (let item of message) { %>
            <tr>
                <td><%= item.name %></td>
                <td><%= item.phone %></td>
                <td><%= item.message %></td> 
                <td><%= moment(item.date).format("DD-MM-YYYY") %></td> 
            </tr> 
            <%}%> 
        </tbody>
    </table> 
</div>