<!-- main start -->
<main class="position-relative overflow-hidden">
  <div id="carusel" class="carusel-home h-100 position-absolute top-0 d-flex">
    <div class="item w-100 h-100 bt-info">
      <img src="./img/home1.jfif" class="w-100 h-100" />
    </div>
    <div class="item w-100 h-100 bt-info">
      <img src="./img/home2.jfif" class="w-100 h-100" />
    </div>
    <div class="item w-100 h-100 bt-info">
      <img src="./img/home3.jfif" class="w-100 h-100" />
    </div>
  </div>
  <div class="audio_play position-absolute w-100 py-2 bottom-0">
    <div class="container text-white">
      <div class="row py-2">
        <div class="col-3 d-none d-lg-block">
          <div class="name">Lalo (Jesica Bernshtein) & Binno Audio Player</div>
        </div>
        <div
          class="col-12 col-lg-9 d-flex justify-content-between align-items-center"
        >
          <audio controls id="mp3" class="d-none">
            <source src="./music/1 Talabalik.mp3" />
          </audio>
          <button
            onclick="prev()"
            class="border border-0 outline-none text-white d-flex align-items-center justify-content-center"
          >
            <i class="fa-solid fa-backward"></i>
          </button>
          <button
            onclick="play()"
            id="home-audio-play"
            class="audio-btn border border-0 outline-none text-white d-flex align-items-center justify-content-center"
          >
            <i class="fa-solid fa-play"></i>
          </button>
          <button
            onclick="next()"
            class="border border-0 outline-none text-white d-flex align-items-center justify-content-center"
          >
            <i class="fa-solid fa-forward"></i>
          </button>
          <div id="home-line" class="line w-75 rounded position-relative">
            <div id="home-line-in" class="line-in h-100 rounded"></div>
            <div
              class="time-mouse position-absolute align-items-center justify-content-center"
            >
              <span id="time-mouse-minute">00</span>:<span
                id="time-mouse-second"
                >00</span
              >
            </div>
          </div>
          <div class="time">
            <span id="home-minute">00</span>:<span id="home-second">00</span>
          </div>
          <i class="fa-solid fa-music d-none d-xl-block"></i>
        </div>
      </div>
    </div>
  </div>
</main>
<!-- main end -->

<!-- about start -->
<section id="about">
  <div class="container">
    <% for (let item of abouts) { %>
    <div class="row py-5 about_in d-flex align-items-center">
      <div class="col-sm-11 col-md-12 col-lg-6">
        <img
          class="img-fluid"
          src="/uploads/about/<%= item.image %>"
          alt="girl.png"
          data-aos="fade-right"
          data-aos-duration="3000"
        />
      </div>
      <div
        class="about_text col-sm-11 col-md-12 col-lg-6 py-4"
        data-aos="fade-left"
        data-aos-duration="3000"
      >
        <span>Biz haqimizda</span>
        <h1 class="about-title"><%= item.title %></h1>
        <p>
          <%= item.description %>
        </p>
        <a class="about_btn" href="#">Batafsil</a>
      </div>
    </div>
    <%}%>
  </div>
</section>
<!-- about end -->

