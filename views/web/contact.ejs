
<!--Start breadcrumb area-->     
<section class="breadcrumb-area breadcrumb-style2-area" style="background-image: url(/images/breadcrumb/breadcrumb-2.jpg);">
    <div class="container">
        <div class="row">
            <div class="col-xl-12"> 
                <div class="inner-content">
                    <div class="title">
                        
                        <% if(lang === 'uz') { %>
                            <h2>
                            <%= trans.hcontact %> 
                            </h2>
                        <% } %> 
                        <% if(lang === 'ru') { %>
                                <h2>
                                <%= trans.hcontact %> 
                                </h2>
                        <% } %> 
                        <% if(lang === undefined) { %>
                                <h2>
                                Aloqa 
                                </h2>
                        <% } %> 
                    </div>
                    <div class="breadcrumb-menu">
                        <ul>
                            <% if(lang === 'uz') { %>
                                <li><a href="/">
                                    <%= trans.hhome %> 
                                </a></li>
                            <% } %> 
                            <% if(lang === 'ru') { %>
                                <li><a href="/">
                                    <%= trans.hhome %> 
                                </a></li>
                            <% } %> 
                            <% if(lang === undefined) { %>
                                <li><a href="/">
                                    Bosh sahifa
                                </a></li>
                            <% } %> 
                            <li><span class="fa fa-angle-right"></span></li>
                            
                            <% if(lang === 'uz') { %>
                                <li class="active">
                                    <%= trans.hcontact %> 
                                <i class="flaticon-right-arrow-1 arrow1"></i></li>
                            <% } %> 
                            <% if(lang === 'ru') { %>
                                <li class="active">
                                    <%= trans.hcontact %> 
                                <i class="flaticon-right-arrow-1 arrow1"></i></li>
                            <% } %> 
                            <% if(lang === undefined) { %>
                                <li class="active">
                                    Aloqa
                                <i class="flaticon-right-arrow-1 arrow1"></i></li>
                            <% } %> 
                        </ul>    
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<!--End breadcrumb area-->

