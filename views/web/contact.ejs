<!--Start Contact Style1 Area-->
<section class="contact-style1-area" id="izoh">
    <div class="container">
        <div class="row">

            <div class="col-xl-8 col-lg-7">
                <div class="contact-style1_form">
                    <div class="title">
                        <% if(lang === 'uz') { %>
                            <h4>
                                <%= trans.sms %> 
                            </h4>
                        <% } %> 
                        <% if(lang === 'ru') { %>
                            <h4>
                                <%= trans.sms %> 
                            </h4> 
                        <% } %> 
                        <% if(lang === undefined) { %>
                            <h4>
                                Xabar yuborish
                            </h4>
                        <% } %> 
                        <p>Etiam tristique venenatis metus, eget maximus elit mattis et. Suspendisse felis odio,<br> Please Enter Your Comments<span>*</span></p>
                    </div>
                    <div class="contact-form">
                        <form  class="default-form2" action="/contact/add" method="POST">
                            <div class="row">
                                <div class="col-xl-6">
                                    <div class="input-box"> 
                                        <input type="text" name="name" placeholder="<%= trans.fullname %> " required>
                                    </div>
                                </div>
                                <div class="col-xl-6">
                                    <div class="input-box"> 
                                        <input type="number" name="phone" placeholder="<%= trans.yourPhone %> " required>
                                    </div>   
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-xl-12">
                                    <div class="input-box">    
                                        <textarea name="message"   placeholder="<%= trans.yourMessage %> " required></textarea>
                                    </div>
                                    <div class="button-box">
                                        <button class="btn-one" type="submit">
                                            <% if(lang === 'uz') { %>
                                                <span class="txt">
                                                    <%= trans.sms %> 
                                                <i class="flaticon-right-arrow-1 arrow1"></i></span>
                                            <% } %> 
                                            <% if(lang === 'ru') { %>
                                                <span class="txt">
                                                    <%= trans.sms %> 
                                                <i class="flaticon-right-arrow-1 arrow1"></i></span>
                                            <% } %> 
                                            <% if(lang === undefined) { %>
                                                <span class="txt">
                                                    Yuborish
                                                <i class="flaticon-right-arrow-1 arrow1"></i></span>
                                            <% } %> 
                                        </button> 
                                    </div>     
                                </div>
                            </div>

                        </form> 
                    </div>
                </div>
            </div>


        </div>
    </div>
</section>
<!--End Contact Style1 Area-->
